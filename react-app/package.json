{
  "type": "module",
  "name": "starter-kit",
  "version": "0.3.15",
  "description": "Starter project for Vite with React + MUI using TypeScript",
  "author": {
    "name": "Hunter Dorhout",
    "email": "dev@codebuddy.com",
    "url": "https://www.codebuddy.com"
  },
  "scripts": {
    "dev": "npx tsc --project ./tsconfig.json && vite --mode development",
    "build": "tsc -b && vite build --mode live",
    "preview": "vite preview --mode live",
    "start": "npm run dev",
    "format": "prettier ./src --write",
    "lint": "eslint .",
    "type": "tsc",
    "check-ts": "tsc --noEmit",
    "test:jest": "cross-env NODE_ENV=test jest",
    "test:vitest": "vitest --mode test",
    "test": "npm run test:vitest"
  },
  "dependencies": {
    "@emotion/react": "11.13.0",
    "@emotion/styled": "11.13.0",
    "@mui/icons-material": "5.16.7",
    "@mui/material": "5.16.7",
    "@mui/x-date-pickers": "7.15.0",
    "axios": "^1.11.0",
    "dayjs": "1.11.13",
    "react": "18.3.1",
    "react-dom": "18.3.1",
    "react-markdown": "9.0.1",
    "react-router-dom": "6.26.1",
    "zustand": "4.5.5",
    "file-saver": "^2.0.5",
    "xlsx": "^0.18.5",
    "recharts": "^2.8.0"
  },
  "devDependencies": {
    "@eslint/js": "9.9.0",
    "@types/node": "^22.12.0",
    "@types/react": "18.3.3",
    "@types/react-color": "3.0.12",
    "@types/react-dom": "18.3.0",
    "@types/react-router-dom": "5.3.3",
    "@types/file-saver": "^2.0.7",
    "@typescript-eslint/eslint-plugin": "8.1.0",
    "@vitejs/plugin-react-swc": "^3.11.0",
    "dotenv": "16.4.5",
    "eslint": "9.9.0",
    "eslint-plugin-react-hooks": "5.1.0-rc-fb9a90fa48-20240614",
    "eslint-plugin-react-refresh": "0.4.9",
    "fs": "0.0.1-security",
    "globals": "15.9.0",
    "jsdom": "24.1.1",
    "prettier": "3.3.3",
    "react-color": "2.19.3",
    "ts-node": "10.9.2",
    "typescript": "5.6.2",
    "typescript-eslint": "8.1.0",
    "vite": "^7.1.3",
    "vite-tsconfig-paths": "5.0.1",
    "vitest": "^3.2.4"
  }
}
